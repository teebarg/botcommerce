{
    "name": "tbo-frontend",
    "version": "1.0.3",
    "private": true,
    "author": "Adeniyi <neyostica2000@yahoo.com> (https://www.niyi.com.ng)",
    "description": "A robust e-commerce storefront built with Next.js 14",
    "keywords": [
        "tbo",
        "nextjs"
    ],
    "scripts": {
        "dev": "next dev --turbo",
        "build": "npm run prettier && next build",
        "start": "next start",
        "lint": "eslint . --ext .ts,.tsx -c .eslintrc.json --fix",
        "analyze": "ANALYZE=true next build",
        "test-e2e": "playwright test e2e",
        "prepare": "husky",
        "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "lint:check": "eslint --max-warnings=50 \"src/**/*.{js,jsx,ts,tsx,json}\" --fix"
    },
    "resolutions": {
        "@types/react": "17.0.40"
    },
    "dependencies": {
        "@headlessui/react": "^1.6.1",
        "@meilisearch/instant-meilisearch": "^0.7.1",
        "@nextui-org/accordion": "^2.0.38",
        "@nextui-org/badge": "^2.0.31",
        "@nextui-org/checkbox": "^2.1.4",
        "@nextui-org/dropdown": "^2.1.29",
        "@nextui-org/image": "^2.0.31",
        "@nextui-org/input": "2.2.4",
        "@nextui-org/kbd": "2.0.33",
        "@nextui-org/listbox": "2.1.25",
        "@nextui-org/modal": "^2.0.39",
        "@nextui-org/navbar": "2.0.36",
        "@nextui-org/pagination": "^2.0.35",
        "@nextui-org/radio": "^2.1.4",
        "@nextui-org/react": "^2.4.6",
        "@nextui-org/scroll-shadow": "^2.1.19",
        "@nextui-org/select": "^2.2.5",
        "@nextui-org/switch": "2.0.33",
        "@nextui-org/system": "2.2.5",
        "@nextui-org/theme": "2.2.9",
        "@nextui-org/tooltip": "^2.0.39",
        "@nextui-org/user": "^2.0.33",
        "@react-aria/ssr": "3.9.4",
        "@react-aria/visually-hidden": "3.8.12",
        "@react-oauth/google": "^0.12.1",
        "algoliasearch": "^4.20.0",
        "clsx": "2.1.1",
        "framer-motion": "^11.3.31",
        "next": "14.2.4",
        "next-themes": "^0.2.1",
        "notistack": "^3.0.1",
        "nui-react-icons": "^0.0.9",
        "pg": "^8.11.3",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-aria": "^3.33.1",
        "react-aria-components": "^1.2.1",
        "react-instantsearch-hooks-web": "^6.29.0",
        "react-pro-sidebar": "^1.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.17.5",
        "@playwright/test": "^1.41.1",
        "@types/node": "20.5.7",
        "@types/nodemailer": "^6.4.15",
        "@types/pg": "^8.11.0",
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
        "@types/react-instantsearch-dom": "^6.12.3",
        "@typescript-eslint/eslint-plugin": "7.2.0",
        "@typescript-eslint/parser": "7.2.0",
        "autoprefixer": "10.4.19",
        "babel-loader": "^8.2.3",
        "eslint": "^8.57.0",
        "eslint-config-next": "14.2.1",
        "eslint-config-prettier": "^8.2.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-unused-imports": "^3.2.0",
        "husky": "^9.0.11",
        "postcss": "8.4.38",
        "prettier": "^3.0.3",
        "tailwind-variants": "0.1.20",
        "tailwindcss": "3.4.3",
        "typescript": "^5.3.2"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --write"
        ],
        "src/**/*.{js,jsx,ts,tsx,json}": [
            "eslint --max-warnings=50 --fix"
        ]
    }
}
